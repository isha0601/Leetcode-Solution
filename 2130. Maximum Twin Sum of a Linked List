/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode() : val(0), next(nullptr) {}
 *     ListNode(int x) : val(x), next(nullptr) {}
 *     ListNode(int x, ListNode *next) : val(x), next(next) {}
 * };
 */
class Solution {
public:
    int pairSum(ListNode* head) {
    vector<int> v;
    ListNode* temp=head;
    while(temp!=NULL){
        v.push_back(temp->val);
        temp=temp->next;
    }
    int start=0;
    int n=v.size();
    int end=n-1;
    int max1=INT_MIN;
    while(start<end){
        int sum=v[start]+v[end];
        if(sum>max1){
            max1=sum;
        }
        start++;
        end--;
    }
    return max1;
    }
};
