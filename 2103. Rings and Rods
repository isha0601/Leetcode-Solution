class Solution {
public:
    int countPoints(string rings) {
        unordered_map<int,set<char>> mp;
        for(size_t i=0;i<rings.size();i+=2){
            char color=rings[i];
            int rod=rings[i+1]-'0';
            mp[rod].insert(color);
        }
        int count=0;
        for(auto& pair:mp){
            if(pair.second.size()==3)count++;
        }
        return count;
    }
};
