class Solution {
public:
    int subarrayBitwiseORs(vector<int>& arr) {
        unordered_set<int> all,prev;
        for(int num:arr){
            unordered_set<int> current;
            current.insert(num);
            for(int val:prev){
                current.insert(val| num);

            } 
            for(int val:current){
                all.insert(val);
            }
            prev=current;
        }
        return all.size();
    }
};
