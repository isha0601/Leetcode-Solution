class Solution {
public:
    string longestCommonPrefix(vector<string>& v) {
        int n=v.size();
        sort(v.begin(),v.end());
        string first=v[0],last=v[n-1];
        string ans="";
        for(int i=0;i<min(first.length(),last.length());i++){
            if(first[i]!=last[i])return ans;
            else ans+=first[i];
        }
        return ans;
    }

};
