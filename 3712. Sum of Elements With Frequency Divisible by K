class Solution {
public:
    int sumDivisibleByK(vector<int>& nums, int k) {
        unordered_map<int,int> mp;    
        for(int val:nums){
            mp[val]++;
        }
        int sum=0;
        for(auto &p:mp){
            if(p.second%k==0)sum+=(p.first*p.second);
        }

        return sum;
    }
};
