class Solution {
public:
    int maxArea(vector<int>& height) {
        int left=0,right=height.size()-1;
        int maxi=0;
        while(left<right){
            int currArea=min(height[left],height[right])*(right-left);
            maxi=max(currArea,maxi);
            if(height[left]<=height[right])left++;
            else right--;
        }
        return maxi;
    }
};
