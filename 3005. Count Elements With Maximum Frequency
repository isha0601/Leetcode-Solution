class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        unordered_map<int,int> mp;
        for(int num:nums){
            mp[num]++;
        }
        int maxFreq=0;
        for(const auto&[key,value]:mp){
            maxFreq=max(value,maxFreq);
        }
        int maxFreCount=0;
        for(const auto& [key,value]:mp){
            if(value==maxFreq){
                maxFreCount++;
            }
        }
        return maxFreCount*maxFreq;
    }
};
